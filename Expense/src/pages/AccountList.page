<apex:page standardController="Account" recordSetVar="accounts">
    <apex:pageBlock >
        <apex:repeat var="a" rendered="true" value="{!accounts}" id="account_list">
            <li>
                
                <apex:outputLink value="/{!a.ID}">
                    <apex:outputText value="{!a.name}"/>
                
                </apex:outputLink> 
            
            </li>
        </apex:repeat>
    
    </apex:pageBlock>
</apex:page>